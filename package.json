{
	"name": "pastefy",
	"publisher": "InteraApps",
	"displayName": "pastefy",
	"description": "A Pastefy VS-Code Addon",
	"version": "1.0.1",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/interaapps/pastefy-vscode"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:pastefy.pasteFromExplorerContextMenu",
		"onCommand:pastefy.pasteFromEditorContextMenu"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pastefy.pasteFromExplorerContextMenu",
				"title": "Pastefy"
			},
			{
				"command": "pastefy.pasteFromEditorContextMenu",
				"title": "Paste Selection to Pastefy"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "pastefy.pasteFromExplorerContextMenu"
				}
			],
			"editor/context": [
				{
					"command": "pastefy.pasteFromEditorContextMenu"
				}
			]
		},
		"configuration": {
			"title": "Pastefy",
			"properties": {
				"pastefy.pastefyAPIKey": {
					"type": "string",
					"default": "",
					"description": "Use your account for pasting. (Get API-Key from https://pastefy.ga/apikeys)"
				}	
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"querystring": "^0.2.0"
	}
}
